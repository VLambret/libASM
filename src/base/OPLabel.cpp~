#include <OPLabel.h>

OPLabel::OPLabel(string op){

    _oper = op;

    }

OPLabel::~OPLabel(){}

 string OPLabel::get_op(){

    return _oper;
    }

t_OpType OPLabel::get_optype(){
   return Lab;
    }


